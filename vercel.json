{
  "buildCommand": "npm run build",
  "outputDirectory": "dist/public",
  "installCommand": "npm install",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "functions": {
    "client/src/**/*.tsx": {
      "runtime": "nodejs18.x"
    }
  },
  "env": {
    "VITE_API_URL": "@api-url"
  }
}
